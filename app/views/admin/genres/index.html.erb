<div class="container">
  <h1>ジャンル一覧（管理者）</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>ジャンル名</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @genres.each do |genre| %>
        <tr>
          <td><%= genre.name %></td>
          <td>
            <%= link_to '詳細', admin_genre_path(genre.id) %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>